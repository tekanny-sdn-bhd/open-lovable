# REQUIRED - Local sandbox daemon
SANDBOXD_URL=http://localhost:4000

# REQUIRED - Web scraping for cloning websites
# Get yours at https://firecrawl.dev
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# OPTIONAL - Override Firecrawl API base URL (default: https://api.firecrawl.dev/v1)
# For self-hosted without auth, point to your local server, e.g. http://localhost:3002/v1
FIRECRAWL_BASE_URL=https://api.firecrawl.dev/v1
# OPTIONAL - Disable auth header for self-hosted instances without API key
FIRECRAWL_DISABLE_AUTH=false

# OPTIONAL - AI Providers (need at least one)
# Get yours at https://console.anthropic.com
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Get yours at https://platform.openai.com
OPENAI_API_KEY=your_openai_api_key_here

# Get yours at https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Get yours at https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# Ollama endpoints
# If using OpenAI-compatible API, include the /v1 path or it will be appended automatically
OLLAMA_LLM_URL=http://192.168.2.3:11434
# Optional: API key placeholder (Ollama typically doesn't require one)
OLLAMA_API_KEY=ollama
OLLAMA_EMBED_URL=http://192.168.2.3:11435
